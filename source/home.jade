							.b-user-wall-footer
								.b-user-wall-footer__image
									a(href="")
										img(src="img/40.png", alt="")
								p.b-user-wall-footer__title Красота Кыргызского народа
								p.b-user-wall-footer__number 31 топик
										.clear
								.b-user-wall-footer__btn
									a.about-btn.btn(href="") Подробнее
									ul.b-user-wall-footer-list
										li
											a.share-btn.btn(href="") Поделиться
