							a(href="#") Загрузить фото

					.b-user-profile__middle
						p.user-raiting Рейтинг
							span.num +0.00
						p.user-name Ярослав Александрович </br>Маркин
						p.user-date 10 декабря 1990

					.b-user-profile__right
						.b-user-profile-link
							a.b-user-profile-link__create(href="#") Создать
						ul.b-user-profile-links
							li.b-user-profile-links__list
								a(href="#")
							li.b-user-profile-links__list
